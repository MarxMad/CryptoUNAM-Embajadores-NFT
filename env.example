# Configuración de Red Sepolia
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
PRIVATE_KEY=your_private_key_here

# Configuración de Etherscan (opcional)
ETHERSCAN_API_KEY=your_etherscan_api_key

# Configuración de WalletConnect (para RainbowKit)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Dirección del contrato desplegado (actualizar después del despliegue)
NEXT_PUBLIC_CONTRACT_ADDRESS=0x...

# Configuración de Alchemy (opcional)
NEXT_PUBLIC_ALCHEMY_ID=your_alchemy_id
